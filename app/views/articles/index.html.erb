<h1>All Articles</h1>
<ul id="articles">
	<% @articles.each do |article| %>
	<li>
		<%= link_to article.id.to_s + " : " + article.title, article_path(article), class:"article_title", id:"article_link#{article.id}" %>
	</li>
	<% end %>
</ul>
<%= link_to 'Create New Article', new_article_path, class:"new_article" %>